<%= button_to "See all Images", images_path, method: :get %>

<div class="container">
  <% flash.each do |key, value| %>
    <div class="alert alert-<%= key %>"><%= value %></div>
  <% end %>
</div>
<%= image_tag(@image_url) %>
<% if @image.tag_list.present? %>
    <p>Tags List</p>
<% else %>
    <p></p>
<% end %>
<ul class='js-tag_list'>
  <%= render partial: 'images/tag_list', locals: { image: @image } %>
</ul>
