<h1>All Images</h1>
<% flash.each do |key, value| %>
  <div class="alert alert-<%= key %>"><%= value %></div>
<% end %>
<ul class='js-image_list'>
  <% @images.each do |image| %>
    <% if image.tag_list.present? %>
      <p>Tags List</p>
    <% else %>
      <p></p>
    <% end %>

      <ul class='js-tag_list'>
        <%= render partial: 'images/tag_list', locals: { image: image } %>
    </ul>
    <li class='js-image_list_element'><%= image_tag(image.url, 'max-width': '400px') %></li>
<% end %>
</ul>
